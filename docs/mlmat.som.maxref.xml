	<attributelist>

        <attribute name='neighborhood' get='1' set='1' type='long' size='1'>
			<digest>Neighborhood radius</digest>
            <description>Neighborhood radius. Zero means use full map radius  (default = 0)           </description>
		</attribute>

        <attribute name='autotrain' get='1' set='1' type='long' size='1'>
			<digest></digest>
            <description> (default = 0)           </description>
		</attribute>

        <attribute name='batch_process' get='1' set='1' type='long' size='1'>
			<digest>Will use batch variant of som algorithm</digest>
            <description>Will use batch variant of som algorithm.  (default = 0)           </description>
		</attribute>

        <attribute name='initialization' get='1' set='1' type='symbol' size='1'>
			<digest>Set how initialization of SOM is performed</digest>
            <description>Set how initialization of SOM is performed. uniform uses uniform random between 0-1, gaussian uses a guassian random distribution and sample will use randomly sampled values from the input matrix.  (default = uniform)           </description>
           <attributelist>
               <attribute name='enumvals' get='1' set='1' type='atom' size='3'>
                   <enumlist>
                       <enum name="uniform">
                       </enum>
                       <enum name="gaussian">
