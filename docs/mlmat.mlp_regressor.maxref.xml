<c74object name='mlmat.mlp_regressor' category='ML'>

	<digest>Multi layer perceptron</digest>
	<description>Multi layer perceptron. This mlp can be used as a regressor. </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Todd Ingalls </metadata>
		<metadata name='tag'>ML</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

	</objarglist>


