<c74object name='mlmat.hmm' category='ML'>

	<digest>Hidden Markov Model</digest>
	<description>Hidden Markov Model. An implementation Hidden Markov Model (HMM). The model can be based on one of four basic types set by <at>hmm_type</at>. </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Todd Ingalls </metadata>
		<metadata name='tag'>ML</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='generate'>
			<digest>Generate points based on distribution</digest>
			<description>Generate points based on distribution. </description>
		</method>

		<method name='clear'>
			<digest>clear previous training input</digest>
			<description>clear previous training input. </description>
		</method>

		<method name='read'>
			<digest></digest>
			<description></description>
		</method>

		<method name='train'>
			<digest>train model</digest>
			<description>train model. </description>
		</method>

		<method name='write'>
			<digest></digest>
			<description></description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

        <attribute name='gaussians' get='1' set='1' type='long' size='1'>
			<digest>Number of gaussians in each GMM (necessary when type is 'gmm'</digest>
            <description>Number of gaussians in each GMM (necessary when type is 'gmm'.  (default = 10)           </description>
		</attribute>

        <attribute name='tolerance' get='1' set='1' type='float64' size='1'>
			<digest>Tolerance of the Baum-Welch algorithm</digest>
            <description>Tolerance of the Baum-Welch algorithm.  (default = 0.00001)           </description>
		</attribute>

        <attribute name='use_labels' get='1' set='1' type='long' size='1'>
			<digest>Use labels in training phase</digest>
            <description>Use labels in training phase. The labels must match the number of training samples. Labels must be used when using <at>hmm_type</at> diag_gmm or gmm.  (default = 0)           </description>
		</attribute>

        <attribute name='autoscale' get='1' set='1' type='long' size='1'>
			<digest>Automatically scale data based on scaling_type</digest>
            <description>Automatically scale data based on scaling_type.  (default = 0)           </description>
		</attribute>

        <attribute name='file' get='1' set='1' type='symbol' size='1'>
			<digest>File </digest>
            <description>File            </description>
		</attribute>

        <attribute name='scaler_epsilon' get='1' set='1' type='float64' size='1'>
			<digest>Epsilon value</digest>
            <description>Epsilon value. Epsilon value.  (default = 0.00005)           </description>
		</attribute>

        <attribute name='seed' get='1' set='1' type='long' size='1'>
			<digest>Random seed</digest>
            <description>Random seed. 0 indicates no seed.  (default = 0)           </description>
		</attribute>

        <attribute name='scaler_max' get='1' set='1' type='long' size='1'>
			<digest>Maximum value</digest>
            <description>Maximum value. Maximum value.  (default = 1)           </description>
		</attribute>

        <attribute name='scaler' get='1' set='1' type='symbol' size='1'>
			<digest>The scaler type</digest>
            <description>The scaler type.  (default = normalization)           </description>
           <attributelist>
               <attribute name='enumvals' get='1' set='1' type='atom' size='6'>
                   <enumlist>
                       <enum name="standard">
                       </enum>
                       <enum name="min_max">
                       </enum>
                       <enum name="normalization">
                       </enum>
                       <enum name="abs">
                       </enum>
                       <enum name="pca_whitening">
                       </enum>
                       <enum name="zca_whitening">
                       </enum>
                   </enumlist>
               </attribute>
               <attribute name='label' get='1' set='1' type='symbol' size='1' value='scaler' />
               <attribute name='style' get='1' set='1' type='symbol' size='1' value='enum' />
           </attributelist>

		</attribute>

        <attribute name='states' get='1' set='1' type='long' size='1'>
			<digest>Number of hidden states in HMM </digest>
            <description>Number of hidden states in HMM  (default = 10)           </description>
		</attribute>

        <attribute name='hmm_type' get='1' set='1' type='symbol' size='1'>
			<digest>Type of HMM </digest>
            <description>Type of HMM  (default = gaussian)           </description>
           <attributelist>
               <attribute name='enumvals' get='1' set='1' type='atom' size='4'>
                   <enumlist>
                       <enum name="discrete">
                       </enum>
                       <enum name="gaussian">
                       </enum>
                       <enum name="diag_gmm">
                       </enum>
                       <enum name="gmm">
                       </enum>
                   </enumlist>
               </attribute>
               <attribute name='label' get='1' set='1' type='symbol' size='1' value='hmm_type' />
               <attribute name='style' get='1' set='1' type='symbol' size='1' value='enum' />
           </attributelist>

		</attribute>

        <attribute name='autoclear' get='1' set='1' type='long' size='1'>
			<digest>Clear training data from memory after the model has been trained</digest>
            <description>Clear training data from memory after the model has been trained.  (default = 1)           </description>
		</attribute>

        <attribute name='scaler_min' get='1' set='1' type='long' size='1'>
			<digest>Minimum value</digest>
            <description>Minimum value. Minimum value.  (default = 0)           </description>
		</attribute>

        <attribute name='mode' get='1' set='1' type='long' size='1'>
			<digest>Input mode for matrices</digest>
            <description>Input mode for matrices.  (default = 0)           </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='mlmat.gmm' />
	</seealsolist>


    <!--DISCUSSION-->

</c74object>
